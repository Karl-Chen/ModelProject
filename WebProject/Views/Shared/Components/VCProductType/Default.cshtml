@model IEnumerable<WebProject.Models.ProductSpecification>

@{
    Layout = null;
}

@* //2.3.9 依喜好編輯Default View排版方式 *@
@if (Model.Count() > 0)
{
    <h3 class="text-left">商品類別</h3>
    <div>
        @foreach (var item in Model)
        {
            <a class="list-group-item text-left" asp-action="Index" asp-route-SpecificationID="@item.SpecificationID">@(item.SpecificationName)</a>
        }
    </div>
}